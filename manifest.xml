      <DesktopSettings>
        <SourceLocation DefaultValue="https://clemens70.github.io/outlook-kalender-addin/calendar-selector-addin.html" />
        <RequestedHeight>300</RequestedHeight>
      </DesktopSettings>
    </Form>
    <Form xsi:type="AppointmentOrganizer">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://clemens70.github.io/outlook-kalender-addin/calendar-selector-addin.html" />
        <RequestedHeight>300</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadWriteItem</Permissions>
